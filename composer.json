{
    "name": "tafhyseni/godaddy-php",
    "description": "A minimalist Godaddy Api Client for domain purchases and administrations",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mustafe Hyseni",
            "email": "tafhyseni@gmail.com"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": ">=6.5",
        "jeremykendall/php-domain-parser": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "pestphp/pest": "^0.1.5"
    },
    "autoload": {
        "psr-4": {
            "Tafhyseni\\PhpGodaddy\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tafhyseni\\PhpGodaddy\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
